{
  "manifestVersion": 1,
  "id": "azuredevops-variables-tasks",
  "publisher": "BastienPerdriau",
  "version": "0.1.1",
  "name": "Variables",
  "scopes": ["vso.build_execute", "vso.release_manage"],
  "description": "Build & Release tasks to handle variable in definitions and executions",
  "galleryFlags": [
    "Preview"
  ],
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "demands": [
    "api-version/3.0"
  ],
  "tags": [
    "Variables"
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "screenshots": [
    {
      "path": "static/images/screen1.png"
    }
  ],
  "content": {
    "license": {
      "path": "LICENSE"
    },
    "details": {
      "path": "overview.md"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/BastienPerdriau/azuredevops-variables-tasks"
    },
    "support": {
      "uri": "https://github.com/BastienPerdriau/azuredevops-variables-tasks/issues"
    },
    "issues": {
      "uri": "https://github.com/BastienPerdriau/azuredevops-variables-tasks/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/BastienPerdriau/azuredevops-variables-tasks"
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "VariableAssignmentTask"
    },
    {
      "path": "VariableIncrementationTask"
    },
    {
      "path": "VariableDefinitionUpdateTask"
    }
  ],
  "contributions": [
    {
      "id": "VariableAssignmentTask",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "VariableAssignmentTask"
      }
    },
    {
      "id": "VariableIncrementationTask",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "VariableIncrementationTask"
      }
    },
    {
      "id": "VariableDefinitionUpdateTask",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "VariableDefinitionUpdateTask"
      }
    }
  ]
}